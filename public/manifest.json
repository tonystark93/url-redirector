{
    "manifest_version": 3,
    "name": "URL Redirect",
    "description": "Redirect URLs including JS, CSS, AJAX calls, and iframes based on custom rules",
    "version": "1.0.0",
    "permissions": [
        "declarativeNetRequest",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "options_page": "options/index.html",
    "action": {
        "default_popup": "popup/index.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        }
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}